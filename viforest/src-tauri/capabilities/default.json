{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "http:default",
    {
      "identifier": "http:default",
      "allow": [
      { "url": "http://*:*" }
      ]
    },
    "fs:write-all",
    {
      "identifier": "fs:scope",
      "allow": [{ "path": "$DOWNLOAD/*" }]
    }
  ]
}